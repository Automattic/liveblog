{
  "parser": "@babel/eslint-parser",
  "extends": [
	"eslint:recommended",
	"plugin:react/recommended"
  ],
  "plugins": [
	"jest",
	"react"
  ],
  "globals": {
    "__DEV__": true,
    "__TEST__": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true,
	"es6": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}
